{
  "name": "ladder_entspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:next": "next dev",
    "dev:websocket": "node websocket-server.js",
    "dev:separate": "concurrently \"npm run dev:next\" \"npm run dev:websocket\"",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "start:websocket": "node websocket-server.js",
    "deploy": "npm run build && npm run db:deploy",
    "db:deploy": "npx prisma generate && npx prisma db push",
    "postbuild": "npx prisma generate",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:init": "node init-database.js",
    "db:studio": "node start-studio.js",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma db push --force-reset"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.8.2",
    "@types/pg": "^8.15.4",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "argon2": "^0.43.0",
    "axios": "^1.9.0",
    "framer-motion": "^5.5.6-beta.0",
    "nanoid": "^5.1.5",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "pg": "^8.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.0",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.0",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5"
  }
}
